                no,
            </if>
            <if test="javlibUrl != null">
                javlib_url,
            </if>
            <if test="title != null">
                title,
            </if>
            <if test="date != null">
                date,
            </if>
            <if test="duretion != null">
                duretion,
            </if>
            <if test="rated != null">
                rated,
            </if>
            <if test="systemTime != null">
                system_time,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="no != null">
                #{no,jdbcType=VARCHAR},
            </if>
            <if test="javlibUrl != null">
                #{javlibUrl,jdbcType=VARCHAR},
            </if>
            <if test="title != null">
                #{title,jdbcType=VARCHAR},
            </if>
            <if test="date != null">
                #{date,jdbcType=DATE},
            </if>
            <if test="duretion != null">
                #{duretion,jdbcType=INTEGER},
            </if>
            <if test="rated != null">
